{
  "name": "bungle/framework-pack",
  "description": "Bungle framework Mono repository",
  "type": "library",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "friendsofphp/php-cs-fixer": "^2.16",
    "squizlabs/php_codesniffer": "^3.5",
    "phpmd/phpmd": "^2.8"
  },
  "require": {
    "doctrine/annotations": "^1.8",
    "beberlei/assert": "^3.2",
    "symfony/dependency-injection": "^5.0",
    "symfony/config": "^5.0",
    "symfony/workflow": "^5.0",
    "symfony/security-bundle": "^5.0",
    "doctrine/mongodb-odm-bundle": "^4.1",
    "symfony/yaml": "^5.0",
    "symfony/form": "^5.0",
    "symfony/validator": "^5.0",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Bungle\\Framework\\": "Framework/",
      "Bungle\\FrameworkBundle\\": "BungleBundle/"
    },
    "exclude-from-classmap": [
      "**/Tests/"
    ]
  }
}
