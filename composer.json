{
  "name": "bungle/framework-pack",
  "description": "Bungle framework Mono repository",
  "type": "library",
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "friendsofphp/php-cs-fixer": "^2.16",
    "squizlabs/php_codesniffer": "^3.5",
    "phpmd/phpmd": "^2.8",
    "mockery/mockery": "^1.3"
  },
  "require": {
    "doctrine/annotations": "^1.8",
    "beberlei/assert": "^3.2",
    "symfony/dependency-injection": "^5.0",
    "symfony/config": "^5.0",
    "symfony/workflow": "^5.0",
    "symfony/security-bundle": "^5.0",
    "doctrine/mongodb-odm-bundle": "^4.1",
    "symfony/yaml": "^5.0",
    "symfony/form": "^5.0",
    "symfony/validator": "^5.0",
    "twig/twig": "^3.0",
    "mingyoung/dingtalk": "^2.3",
    "symfony/serializer": "^5.0",
    "symfony/property-info": "^5.0",
    "symfony/property-access": "^5.0",
    "lstrojny/functional-php": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Bungle\\Framework\\": "Framework/src/",
      "Bungle\\FrameworkBundle\\": "BungleBundle/src/",
      "Bungle\\DingTalk\\": "DingTalk/src/"
    },
    "exclude-from-classmap": [
      "*/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bungle\\Framework\\Tests\\": "Framework/tests/",
      "Bungle\\FrameworkBundle\\Tests\\": "BungleBundle/tests/",
      "Bungle\\DingTalk\\Tests\\": "DingTalk/tests/"
    }
  }

}
